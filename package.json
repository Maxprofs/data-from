{
  "name": "@hidoo/data-from",
  "version": "0.1.1",
  "description": "A utility library that get data from JSON, JSON5, and YAML.",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "lib/index.js",
  "files": [
    "lib/",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production babel ./src --out-dir ./lib",
    "test": "npm-run-all -s test:*",
    "test:lint": "eslint .",
    "test:unit": "cross-env NODE_ENV=test mocha ./test",
    "prepare": "npm-run-all -s test build"
  },
  "keywords": [
    "data",
    "yaml",
    "json5"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hidoo/data-from.git"
  },
  "author": "hidoo",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.3",
    "@babel/core": "^7.0.0-rc.3",
    "@babel/preset-env": "^7.0.0-rc.3",
    "@babel/register": "^7.0.0-rc.3",
    "@hidoo/eslint-config": "^0.1.1",
    "babel-eslint": "^10.0.1",
    "babel-preset-power-assert": "^3.0.0-beta.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.4.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "power-assert": "^1.6.0"
  },
  "dependencies": {
    "front-matter": "^2.3.0",
    "glob": "^7.1.3",
    "handlebars": "^4.0.11",
    "js-yaml": "^3.12.0",
    "json5": "^2.0.1",
    "lodash.merge": "^4.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint"
    ]
  }
}
